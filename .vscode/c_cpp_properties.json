{
    "configurations": [
        {
            "name": "saml22",
            "includePath": [
                "${workspaceFolder}/app/inc/",
                "${workspaceFolder}/bootloader/inc/",
                "${workspaceFolder}/dependencies/cmsis_core/**",
                "${workspaceFolder}/dependencies/cmsis_saml22/**",
                "${workspaceFolder}/shared/inc/**"
            ],
            "defines": [
                "__SAML22N18A__",
                "cortex-m0plus",
                "BOARD=SAML22_XPLAINED_PRO_B",
                "ARM_MATH_CM0PLUS=true"
            ],
            "compilerPath": "/usr/bin/arm-none-eabi-gcc",
            "cStandard": "c99",
            "intelliSenseMode": "gcc-arm"
        }
    ],
    "version": 4
}