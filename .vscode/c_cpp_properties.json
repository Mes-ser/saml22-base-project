{
    "configurations": [
        {
            "name": "saml22",
            "includePath": [
                "${workspaceFolder}/app",
                "${workspaceFolder}/app/inc",
                "${workspaceFolder}/dependencies/cmsis_core/CMSIS/Core/Include",
                "${workspaceFolder}/dependencies/cmsis_saml22/saml22/include",
                "${workspaceFolder}/dependencies/cmsis_saml22/saml22/source"

            ],
            "defines": [
                "__SAML22N18A__",
                "cortex-m0plus",
                "BOARD=SAML22_XPLAINED_PRO_B",
                "ARM_MATH_CM0PLUS=true"
            ],
            "compilerPath": "/usr/bin/arm-none-eabi-gcc",
            "cStandard": "c99",
            "intelliSenseMode": "gcc-arm"
        }
    ],
    "version": 4
}